(add|stage) $opt* --? $path+
  --all
  --dry-run
  --edit
  --force
  --ignore-errors
  --ignore-missing
  --ignore-removal
  --intent-to-add
  --interactive
  --no-all
  --no-ignore-removal
  --refresh
  --update
  --verbose

blame $opt* $revision? --? $path
  --incremental
  --line-porcelain
  --no-progress
  --porcelain
  --progress
  --root
  --score-debug
  --show-email
  --show-name
  --show-number
  --show-stats
  --abbrev $anything
  --contents $path
  --date $anything
  --encoding $anything
  --reverse $revision
  --since $anything
  -S $path

# Listing
branch $opt* $branch*
  --no-color
  --no-abbrev
  --no-column
  --list
  --remotes
  --all
  --verbose
  --color (always|never|auto)
  --abbrev $anything
  --column $anything
  --merged $revision
  --no-merged $revision
  --contains $revision
  --sort $anything
  --points-at $anything

# Creating
branch $opt* $branch $revision?
  --set-upstream
  --track
  --no-track
  --force
  --create-reflog
  --quiet

# Modifying
branch $opt $branch?
  --edit-description
  --set-upstream-to $branch
  -u $branch

# Renaming
branch $opt+ $branch? $anything
  --force
  --move
  -M
  -m

# Deleting
branch $opt* $branch+
  --delete
  --force
  --remotes
  --quiet
  -d
  -D

checkout $opt* $revision
  --quiet
  --force
  --merge
  --detach

checkout $opt* $revision?
  --quiet
  --force
  --merge
  --track
  --no-track
  --orphan $anything
  -b $anything
  -B $anything

checkout $opt? $treeish? --? $path+
  --force
  --theirs
  --ours
  --merge
  --confict (merge|diff3)
  --patch

cherry-pick $opt* $revision+
  --edit
  --ff
  --allow-empty
  --allow-empty-message
  --keep-redundant-commits
  --strategy (resolve|recursive|octopus|ours|subtree)
  --strategy-option (ours|theirs|patience|diff-algorithm|ignore-space-change|ignore-all-space|ignore-space-at-eol|renormalize|no-renormalize|no-renames|find-renames|rename-threshold|subtree)
  -X (ours|theirs|patience|diff-algorithm|ignore-space-change|ignore-all-space|ignore-space-at-eol|renormalize|no-renormalize|no-renames|find-renames|rename-threshold|subtree)
  -m $anything

cherry-pick $opt
  --continue
  --quit
  --abort

commit $opt* --? $path*
  --all
  --allow-empty
  --allow-empty-message
  --amend
  --dry-run
  --edit
  --include
  --interactive
  --no-status
  --no-verify
  --only
  --patch
  --reset-author
  --signoff
  --status
  --verbose
  --author $anything
  --cleanup (strip|whitespace|verbatim|scissors|default)
  --date $anything
  --file $path
  --fixup $revision
  --gpg-sign $anything
  --message $anything
  --reedit-message $revision
  --reuse-message $revision
  --squash $revision
  --untracked-files (no|normal|all)?
  -C $revision
  -F $path
  -c $revision
  -m $anything

config $opt*
  --system
  --global
  --local
  --file $path
  --int
  --bool
  --bool-or-int
  --replace-all
  --add
  --get
  --get-all
  --get-regexp
  --get-urlmatch $anything
  --blob $treeish
  --remove-section
  --rename-section
  --unset
  --unset-all
  --list
  --path
  --null
  --name-only
  --show-origin
  --get-colorbool $anything
  --get-color $anything
  --edit
  --includes
  --no-includes
  -f $path

diff $opt* ($revision|$revision $revision)? --? $path*
  --binary
  --cached
  --check
  --compaction-heuristic
  --exit-code
  --ext-diff
  --find-copies-harder
  --full-index
  --function-context
