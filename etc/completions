  --init
  --remote
  --no-fetch
  --force
  --rebase
  --merge
  --reference $remote
  --depth $anything
  --recursive
  --jobs $anything

submodule --quiet? foreach $opt* $anything
  --recursive

submodule --quiet? sync $opt* --? $dir_path+
  --recursive

tag $opt* $tag $revision
  --annotate
  --sign
  --local-user $anything
  --force
  --delete
  --verify
  --list $tag
  --sort $anything
  --column $anything
  --no-column
  --contains $revision
  --points-at $revision
  --message $anything
  --file $path
  --cleanup (strip|whitespace|verbatim)
  --create-reflog
  --merged $revision?
  --no-merged
  -l $tag
  -m $anything
  -F $path


