add $opt* --? $path+
  --all
  --dry-run
  --edit
  --force
  --ignore-errors
  --ignore-missing
  --ignore-removal
  --intent-to-add
  --interactive
  --no-all
  --no-ignore-removal
  --refresh
  --update
  --verbose

blame $opt* $revision? --? $path
  --incremental
  --line-porcelain
  --no-progress
  --porcelain
  --progress
  --root
  --score-debug
  --show-email
  --show-name
  --show-number
  --show-stats
  --abbrev $anything
  --contents $path
  --date $anything
  --encoding $anything
  --reverse $revision
  --since $anything
  -S $path

# Listing
branch $opt* $branch*
  --no-color
  --no-abbrev
  --no-column
  --list
  --remotes
  --all
  --verbose
  --color (always|never|auto)
  --abbrev $anything
  --column $anything
  --merged $revision
  --no-merged $revision
  --contains $revision
  --sort $anything
  --points-at $anything

# Creating
branch $opt* $branch $revision?
  --set-upstream
  --track
  --no-track
  --force
  --create-reflog
  --quiet

# Modifying
branch $opt $branch?
  --edit-description
  --set-upstream-to $branch
  -u $branch

# Renaming
branch $opt+ $branch? $anything
  --force
  --move
  -M
  -m

# Deleting
branch $opt* $branch+
  --delete
  --force
  --remotes
  --quiet
  -d
  -D

bundle create $path                   # followed by git-rev-list-arguments
bundle verify $path
bundle list-heads $path $revision*
bundle unbundle $path $revision*

checkout $opt* $revision
  --quiet
  --force
  --merge
  --detach

checkout $opt* $revision?
  --quiet
  --force
  --merge
  --track
  --no-track
  --orphan $anything
  -b $anything
  -B $anything
  -t

checkout $opt? $treeish? --? $path+
  --force
  --theirs
  --ours
  --merge
  --confict (merge|diff3)

checkout $opt $treeish? --? $path*
  --patch
  -p

commit $opt* --? $path*
  --all
  --allow-empty
  --allow-empty-message
  --amend
  --dry-run
  --edit
  --include
  --interactive
  --no-status
  --no-verify
  --only
  --patch
  --reset-author
  --signoff
  --status
  --verbose
  --author $anything
  --cleanup (strip|whitespace|verbatim|scissors|default)
  --date $anything
  --file $path
  --fixup $revision
  --gpg-sign $anything
  --message $anything
  --reedit-message $revision
  --reuse-message $revision
  --squash $revision
  --untracked-files (no|normal|all)?
  -C $revision
  -F $path
  -c $revision
  -m $anything

diff $opt* ($revision|$revision $revision|$range)? --? $path*
  --binary
  --cached
  --check
  --compaction-heuristic
  --exit-code
  --ext-diff
  --find-copies-harder
  --full-index
  --function-context
  --histogram
  --ignore-all-space
  --ignore-blank-lines
  --ignore-space-at-eol
  --ignore-space-change
  --irreversible-delete
  --minimal
  --name-only
  --name-status
  --no-color
  --no-compaction-heuristic
  --no-ext-diff
  --no-index
  --no-patch
  --no-prefix
  --no-renames
  --no-textconv
  --numstat
  --patch
  --patch-with-raw
  --patch-with-stat
  --patience
  --pickaxe-all
  --pickaxe-regex
  --quiet
  --raw
  --shortstat
  --summary
  --text
  --textconv
  --abbrev $anything?
  --break-rewrites $anything?
  --color (always|never|auto)
  --color-words $anything?
  --diff-algorithm (default|patient|minimal|histogram|myers)
  --diff-filter $anything?
  --dirstat {changes,lines,files,cumulative}?
  --dst-prefix $anything
  --find-copies $anything?
  --find-renames $anything?
  --ignore-submodules (none|untracked|dirty|all)?
  --inter-hunk-context $anything
  --relative $path?
  --src-prefix $anything
  --stat $anything?
  --submodule $anything   # Maybe there should be a list here?
  --unified $anything
  --word-diff (color|plain|porcelain|none)?
  --word-diff-regex $anything
  --ws-error-highlight {old,new,context}

log $opt* $range? --? $path*
  --follow
  --no-deocrate
  --decorate (short|full|auto|no)
  --source
  --use-mailmap
  --full-diff
  --log-size
  --skip $anything
  --since $anything
  --after $anything
  --until $anything
  --before $anything
  --author $anything
  --committer $anything
  --grep-reflog $anything
  --grep $anything
  --all-match
  --invert-grep
  --regexp-ignore-case
  --basic-regexp
  --extended-regexp
  --fixed-strings
  --perl-regexp
  --remove-empty
  --merges
  --no-merges
  --min-parents $anything
  --max-parents $anything
  --no-min-parents
  --no-max-parents
  --first-parent
  --not
  --all
  --branches $branch?
  --tags $anything?
  --remotes $remote?
  --glob $anything
  --exclude $anything
  --reflog
  --ignore-missing
  --bisect
  --stdin
  --cherry-mark
  --cherry-pick
  --left-only
  --right-only
  --cherry
  --walk-reflogs
  --merge
  --boundary
  --simplify-by-decoration
  --full-history
  --dense
  --sparse
  --simplify-merges
  --ancestry-path
  --date-order
  --author-date-order
  --topo-order
  --reverse
  --no-walk (sorted|unsorted)?
  --do-walk
  --pretty (oneline|short|medium|full|fuller|email|raw|format:)?
  --format (oneline|short|medium|full|fuller|email|raw|format:)
  --abbrev-commit
  --no-abbrev-commit
  --oneline
  --encoding $anything
  --expand-tabs $anything?
  --no-expand-tabs
  --notes $treeish?
  --no-notes
  --show-notes $treeish?
  --standard-notes
  --no-standard-notes
  --show-signature
  --relative-date
  --date $anything
  --parents
  --children
  --left-right
  --graph
  --show-linear-break $anything?
  -L $anything
  -n $anything


# gitsh internal commands
# =======================

:set

:cd $dir_path

:exit

:echo

:help (:set|:cd|:exit|:echo|:help|:source)

:source $path


