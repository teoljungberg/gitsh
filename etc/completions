submodule --quiet? add $opt* --? $remote $dir_path
  --branch $branch
  --force
  --name $anything
  --reference $remote
  --depth $anything

submodule --quiet? status $opt* --? $dir_path+
  --cached
  --recursive

submodule --quiet? init --? $dir_path+

submodule --quiet? deinit $opt* --? $dir_path+
  --force
  --all

